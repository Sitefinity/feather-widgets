@model Telerik.Sitefinity.DynamicModules.Builder.Model.DynamicModuleField

@@*Start @Model.Name field*@@
<ul>
@@foreach (var relatedItem in Model.Item.Fields.@(Model.Name))
{
    <li class="media">
        <a class="media-body" href='@@relatedItem.Fields.MediaUrl'>
            <img src='@@relatedItem.Fields.ThumbnailUrl' alt='@@relatedItem.Fields.AlternativeText' title='@@relatedItem.Fields.Title' />
        </a>
    </li>   
}
</ul>
@@*End @Model.Name field*@@