@model Telerik.Sitefinity.DynamicModules.Builder.Model.DynamicModuleField

@using Telerik.Sitefinity.Frontend.DynamicContent.WidgetTemplates.Fields.Helpers;

@@*Start @Model.Name field*@@
<div>
    <strong> @Model.FrontendWidgetLabel :</strong>

    <ul>
    @@foreach (Telerik.Sitefinity.Frontend.Mvc.Models.ItemViewModel pageItem in Model.Item.Fields.@(Model.Name))
    {
        <li>
            <a @@pageItem.LinkTargetAttribute() href='@@pageItem.DefaultUrl'>
                @@pageItem.Fields.Title
            </a>
        </li>   
    }
    </ul>
</div>
@@*End @Model.Name field*@@